{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "ingestion": {
    "blackList": "BLACK_LIST"
  },
  "provider": {
    "ingestion": {
      "type": "PROVIDER_FROM",
      "accessKeyId": "S3_SOURCE_ACCESS_KEY_ID",
      "secretAccessKey": "S3_SOURCE_SECRET_ACCESS_KEY",
      "endpointUrl": "S3_SOURCE_END_POINT_URL",
      "bucket": "S3_SOURCE_BUCKET",
      "region": "S3_SOURCE_REGION",
      "forcePathStyle": {
        "__name": "S3_SOURCE_FORCE_PATH_STYLE",
        "__format": "boolean"
      },
      "sslEnabled": {
        "__name": "S3_SOURCE_SSL_ENABLED",
        "__format": "boolean"
      },
      "maxAttempts": {
        "__name": "S3_SOURCE_MAX_ATTEMPTS",
        "__format": "number"
      },
      "pvPath": "PV_SOURCE_PATH"
    },
    "delete": {
      "type": "PROVIDER_TO",
      "accessKeyId": "S3_DEST_ACCESS_KEY_ID",
      "secretAccessKey": "S3_DEST_SECRET_ACCESS_KEY",
      "endpointUrl": "S3_DEST_END_POINT_URL",
      "bucket": "S3_DEST_BUCKET",
      "region": "S3_DEST_REGION",
      "forcePathStyle": {
        "__name": "S3_DEST_FORCE_PATH_STYLE",
        "__format": "boolean"
      },
      "sslEnabled": {
        "__name": "S3_DEST_SSL_ENABLED",
        "__format": "boolean"
      },
      "maxAttempts": {
        "__name": "S3_DEST_MAX_ATTEMPTS",
        "__format": "number"
      },
      "pvPath": "PV_DEST_PATH"
    }
  },
  "jobManager": {
    "url": "JOB_MANAGER_URL",
    "job": {
      "type": {
        "ingestion": "INGESTION_JOB_TYPE",
        "delete": "DELETE_JOB_TYPE"
      }
    },
    "task": {
      "batches": {
        "__name": "TASK_BATCHES",
        "__format": "number"
      },
      "type": {
        "ingestion": "INGESTION_TASK_TYPE",
        "delete": "DELETE_TASK_TYPE"
      }
    }
  },
  "maxConcurrency": "MAX_CONCURRENCY"
}
