{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": true,
      "url": "http://localhost:4318/v1/traces"
    },
    "metrics": {
      "enabled": true,
      "buckets": [0.005, 0.01, 0.025, 0.05, 0.1, 0.25, 0.5, 1, 2.5, 5, 10, 15, 50, 250, 500]
    }
  },
  "server": {
    "port": 8082,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "storage": {
    "provider": "NFS",
    "config": {
      "pvPath": "fda"
    }
  },
  "blackList": ["tar", "zip", "rar", "7z"],
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "job": {
      "type": "Ingestion_New_3D"
    },
    "task": {
      "batches": 100,
      "type": "tilesCopying"
    }
  },
  "maxConcurrency": 5
}
