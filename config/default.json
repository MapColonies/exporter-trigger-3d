{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8081",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "ingestion": {
    "blackList": ["tar", "zip", "rar", "7z"]
  },
  "provider": {
    "ingestion": {
      "type": "S3",
      "accessKeyId": "raster",
      "secretAccessKey": "rasterPassword",
      "endpointUrl": "http://10.8.0.9:9000",
      "bucket": "3dtiles",
      "region": "ui-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3
    },
    "delete": {
      "type": "S3",
      "accessKeyId": "raster",
      "secretAccessKey": "rasterPassword",
      "endpointUrl": "http://10.8.0.9:9000",
      "bucket": "3dtiles",
      "region": "ui-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3
    }
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "job": {
      "type": {
        "ingestion": "Ingestion_New_3D",
        "delete": "Delete_3D_Model"
      }
    },
    "task": {
      "batches": 100,
      "type": {
        "ingestion": "tilesCopying",
        "delete": "tilesDeleting"
      }
    }
  },
  "maxConcurrency": 5
}
