{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8081",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "S3": {
    "accessKeyId": "minio",
    "secretAccessKey": "minio",
    "endpointUrl": "http://10.8.0.9:9000",
    "bucket": "3dtiles-dest",
    "region": "us-east-1",
    "forcePathStyle": true,
    "sslEnabled": false,
    "maxAttempts": 3
  },
  "NFS": {
    "pvPath": "/home/netanel/3DModels"
  },
  "ingestion": {
    "provider": "NFS",
    "blackList": ["tar", "zip", "rar", "7z"]
  },
  "provider": {
    "ingestion": {
      "type": "NFS",
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "3dtiles",
      "region": "aws-global",
      "forcePathStyle": true,
      "tls": false,
      "maxAttempts": 4,
      "sigVersion": "v4"
    },
    "delete": {
      "type": "S3",
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "3dtiles",
      "region": "aws-global",
      "forcePathStyle": true,
      "tls": false,
      "maxAttempts": 4,
      "sigVersion": "v4",
      "storageClass": "STANDARD"
    }
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "job": {
      "type": {
        "ingestion": "Ingestion_New_3D",
        "delete": "Delete_3D_Model"
      }
    },
    "task": {
      "batches": 100,
      "type": {
        "ingestion": "tiles_copying",
        "delete": "tiles_deleting"
      }
    }
  },
  "maxConcurrency": 5
}
