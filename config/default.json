{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "debug",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8081",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "S3": {
    "accessKeyId": "vector",
    "secretAccessKey": "vectorPassword",
    "endpointUrl": "http://10.8.0.9:9000",
    "bucket": "3dtilesint",
    "region": "us-east-1",
    "forcePathStyle": true,
    "sslEnabled": false,
    "maxAttempts": 3
  },
  "NFS": {
    "pvPath": "~/Downloads"
  },
  "ingestion": {
    "provider": "NFS",
    "queueFilePath": "queueFile.txt",
    "blackList": ["tar", "zip", "rar", "7z"]
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080"
  },
  "fileSyncer": {
    "maxRequests": 5,
    "job": {
      "type": "Ingestion_New_3D"
    },
    "task": {
      "batches": 100,
      "type": "Ingestion"
    }
  }
}
