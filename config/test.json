{
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpointUrl": "http://127.0.0.1:9000",
    "bucket": "test",
    "region": "us-east-1",
    "forcePathStyle": true,
    "sslEnabled": false,
    "maxAttempts": 4
  },
  "NFS": {
    "pvPath": "./tests/helpers/3DModels"
  },
  "ingestion": {
    "provider": "NFS",
    "blackList": ["tar", "zip", "rar", "7z"]
  },
  "provider": {
    "ingestion": {
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "test",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4",
      "storageClass": "STANDARD"
    },
    "delete": {
      "accessKeyId": "minioadmin",
      "secretAccessKey": "minioadmin",
      "endpointUrl": "http://127.0.0.1:9000",
      "bucket": "test",
      "region": "us-east-1",
      "forcePathStyle": true,
      "sslEnabled": false,
      "maxAttempts": 3,
      "sigVersion": "v4",
      "storageClass": "STANDARD"
    }
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "job": {
      "type": {
        "ingestion": "Ingestion_New_3D",
        "delete": "Delete_3D_Model"
      }
    },
    "task": {
      "batches": 5,
      "type": {
        "ingestion": "tiles_copying",
        "delete": "tiles_deleting"
      }
    }
  },
  "maxConcurrency": 5
}
